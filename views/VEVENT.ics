BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//hacksw/handcal//NONSGML v1.0//EN
BEGIN:VEVENT
UID:{{ uid }}
DTSTAMP:{{ stamp | iso8601 }}{% if start | exists %}
DTSTART:{{ start | iso8601 }}{% if end | exists %}
DTEND:{{ end | iso8601 }}{% endif %}{% endif %}{% if description | exists %}
